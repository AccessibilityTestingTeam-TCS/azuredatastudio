				{
					"id": "cmsResourceExplorer",
					"name": "%cms.resource.explorer.title%"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "cms.resource.registerCMSServer",
					"when": "view == cmsResourceExplorer",
					"group": "navigation"
				}
			],
